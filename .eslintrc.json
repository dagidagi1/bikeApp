{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true,
        "browser": true
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "globals":{
        "document": false
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "max-len": "off",
        "new-cap": ["error", {"capIsNew": false}],
        "prefer-rest-params": "off",
        "prefer-spread": "off",
        "no-invalid-this":"off",
        "require-jsdoc": "off",
        "no-var": "off",
        "quotes": [
            "error",
            "single"]
    }
}
